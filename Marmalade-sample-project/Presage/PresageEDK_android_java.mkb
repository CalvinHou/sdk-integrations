#!/usr/bin/env mkb
# Builder mkb file for the java portion of the PresageEDK extension on android
platform JAVA

files
{
    (source/android)
    PresageEDK.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

librarypath "lib/presage-lib-1.8.1.jar"

option output-name=lib/android/PresageEDK.jar
